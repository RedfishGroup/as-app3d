{
  "name": "@redfish/as-app3d",
  "version": "0.3.0",
  "description": "AgentScript, Model + View application",
  "author": "Owen Densmore",
  "homepage": "http://backspaces.github.io/as-app3d/",
  "license": "GPL-3.0",
  "main": "dist/as-app3d.umd.js",
  "module": "dist/as-app3d.esm.js",
  "files": [
    "dist",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/backspaces/as-app3d.git"
  },
  "keywords": [
    "netlogo",
    "3D",
    "three.js"
  ],
  "wraplibs": [
    [
      "node_modules/three/build/three.min.js",
      "THREE"
    ],
    [
      "node_modules/three/examples/js/controls/OrbitControls.js",
      "THREE.OrbitControls"
    ]
  ],
  "mkdirs": "dist docs/dist docs/models",
  "scripts": {
    "clean": "cross-env-shell \"shx rm -rf $npm_package_mkdirs && shx mkdir $npm_package_mkdirs\"",
    "build": "npm-run-all clean build-libs build-dist minify build-docs",
    "build-libs": "node bin/wraplibs.js",
    "build-dist": "rollup -c",
    "build-docs": "shx cp -R README.md dist docs && node bin/docsmodels.js",
    "minify": "squash dist/as-app3d.umd.js > dist/as-app3d.umd.min.js && squash dist/as-app3d.esm.js > dist/as-app3d.esm.min.js",
    "test": "ava --verbose",
    "foo": "files='one \"two three\"' && shx echo $files"
  },
  "dependencies": {
    "three": "^0.89.0"
  },
  "devDependencies": {
    "@redfish/agentscript": "^0.1.0",
    "ava": "^0.24.0",
    "butternut": "^0.4.6",
    "cross-env": "^5.1.3",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "puppeteer": "^1.0.0",
    "rollup": "^0.53.1",
    "shelljs": "^0.8.0",
    "shx": "^0.2.2"
  }
}
