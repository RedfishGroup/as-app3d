{
  "name": "asx",
  "version": "v0.3.0",
  "description": "AgentScript, 2nd Edition",
  "homepage": "http://backspaces.github.io/asx/",
  "main": "src/app.js",
  "author": "Owen Densmore",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/backspaces/asx.git"
  },
  "keywords": [
    "netlogo",
    "3D",
    "three.js"
  ],
  "libs": [
    "node_modules/three/build/three.min.js",
    "node_modules/three/examples/js/controls/OrbitControls.js",
    "node_modules/stats.js/build/stats.min.js",
    "node_modules/dat.gui/build/dat.gui.min.js"
  ],
  "wraplibs": [
    ["libs/three.min.js", "THREE"],
    ["libs/OrbitControls.js", "THREE.OrbitControls"],
    ["libs/dat.gui.min.js", "dat"],
    ["libs/stats.min.js", "Stats"]
  ],
  "themenames": [
    "Use one of these for 'docstheme' below",
    "jekyll-theme-architect",
    "jekyll-theme-cayman",
    "jekyll-theme-dinky",
    "jekyll-theme-hacker",
    "jekyll-theme-leap-day",
    "jekyll-theme-merlot",
    "jekyll-theme-midnight",
    "jekyll-theme-minima",
    "jekyll-theme-minimal",
    "jekyll-theme-modernist",
    "jekyll-theme-slate",
    "jekyll-theme-tactile",
    "jekyll-theme-time-machine"
  ],
  "docsfiles": [
    "README.md",
    "dist",
    "models"
  ],
  "mkdirs": [
    "docs",
    "dist",
    "libs",
    "models/scripts"
  ],
  "docstheme": "theme: jekyll-theme-hacker",
  "scripts": {
    "clean": "rm -rf `bin/pkgkey.js mkdirs` && npm run mkdirs",
    "mkdirs": "mkdir -p `bin/pkgkey.js mkdirs`",
    "start": "node node_modules/serve/bin/serve.js -p 3000",
    "build": "npm run clean && npm run build-libs && npm run build-models && npm run build-dist && squash dist/AS.js > dist/AS.min.js && squash dist/AS.module.js > dist/AS.module.min.js && npm run build-docs",
    "build-libs": "cp `bin/pkgkey.js libs` libs && bin/wraplibs.js",
    "build-docs": "cp -Rp `bin/pkgkey.js docsfiles` docs && echo `bin/pkgkey.js docstheme` > docs/_config.yml",
    "build-models": "bin/buildscripts.sh models/src",
    "build-dist": "rollup -c && cp -R src dist/AS",
    "watch": "sane 'npm run build-models' './models/src/' & sane 'npm run build-dist' './src/'"
  },
  "dependencies": {
    "dat.gui": "dataarts/dat.gui",
    "stats.js": "^0.17.0",
    "three": "latest"
  },
  "devDependencies": {
    "butternut": "^0.4.5",
    "pako": "^1.0.5",
    "rollup": "^0.41.6",
    "sane": "^1.6.0",
    "serve": "^5.1.4",
    "uglify-es": "^3.0.18"
  }
}
