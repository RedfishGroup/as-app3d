{
  "name": "asx",
  "version": "v0.3.0",
  "description": "AgentScript, 2nd Edition",
  "homepage": "http://backspaces.github.io/asx/",
  "main": "src/app.js",
  "author": "Owen Densmore",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/backspaces/asx.git"
  },
  "keywords": [
    "netlogo",
    "3D",
    "three.js"
  ],
  "libs": [
    "node_modules/three/build/three.min.js",
    "node_modules/three/examples/js/controls/OrbitControls.js",
    "node_modules/stats.js/build/stats.min.js",
    "node_modules/dat.gui/build/dat.gui.min.js"
  ],
  "wraplibs": [
    [
      "libs/three.min.js",
      "THREE"
    ],
    [
      "libs/OrbitControls.js",
      "THREE.OrbitControls"
    ],
    [
      "libs/dat.gui.min.js",
      "dat"
    ],
    [
      "libs/stats.min.js",
      "Stats"
    ]
  ],
  "mkdirs": [
    "src/core",
    "dist",
    "docs/src",
    "docs/dist",
    "docs/models",
    "libs"
  ],
  "scripts": {
    "clean": "rm -rf `bin/pkgkey.js mkdirs` && mkdir `bin/pkgkey.js mkdirs`",
    "build": "yarn run clean && yarn run build-core && yarn run build-libs && yarn run build-dist && yarn run minify && yarn run build-docs",
    "build-libs": "cp `bin/pkgkey.js libs` libs && bin/wraplibs.js",
    "build-docs0": "cp -Rp `bin/pkgkey.js docsfiles` docs",
    "build-docs": "cp -Rp README.md dist docs && bin/docsmodels.sh",
    "build-core": "cp node_modules/as-core/src/*js src/core",
    "minify": "squash dist/AS.js > dist/AS.min.js && squash dist/AS.module.js > dist/AS.module.min.js",
    "build-dist": "rollup -c",
    "test": "pwd #always repo root"
  },
  "dependencies": {
    "dat.gui": "^0.6.5",
    "stats.js": "^0.17.0",
    "three": "^0.89.0"
  },
  "devDependencies": {
    "as-core": "github:backspaces/as-core",
    "butternut": "^0.4.6",
    "rollup": "^0.53.1",
    "sane": "^1.6.0"
  }
}
