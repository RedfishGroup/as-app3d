{
  "name": "asx",
  "version": "v0.3.0",
  "description": "AgentScript, 2nd Edition",
  "homepage": "http://backspaces.github.io/asx/",
  "main": "src/app.js",
  "author": "Owen Densmore",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/backspaces/asx.git"
  },
  "keywords": [
    "netlogo",
    "3D",
    "three.js"
  ],
  "libs": [
    "node_modules/three/build/three.min.js",
    "node_modules/three/examples/js/controls/OrbitControls.js",
    "node_modules/stats.js/build/stats.min.js",
    "node_modules/dat.gui/build/dat.gui.min.js"
  ],
  "wraplibs": [
    [
      "libs/three.min.js",
      "THREE"
    ],
    [
      "libs/OrbitControls.js",
      "THREE.OrbitControls"
    ],
    [
      "libs/dat.gui.min.js",
      "dat"
    ],
    [
      "libs/stats.min.js",
      "Stats"
    ]
  ],
  "docsfiles": [
    "README.md",
    "src",
    "models",
    "dist"
  ],
  "mkdirs": [
    "src/core",
    "dist",
    "docs/src",
    "docs/dist",
    "docs/models",
    "libs"
  ],
  "scripts": {
    "clean": "rm -rf `bin/pkgkey.js mkdirs` && mkdir `bin/pkgkey.js mkdirs`",
    "build": "npm run clean && npm run build-core && npm run build-libs && npm run build-dist && npm run minify && npm run build-docs",
    "build-libs": "cp `bin/pkgkey.js libs` libs && bin/wraplibs.js",
    "build-docs": "cp -Rp `bin/pkgkey.js docsfiles` docs",
    "build-core": "cp node_modules/coreas/src/*js src/core",
    "minify": "squash dist/AS.js > dist/AS.min.js && squash dist/AS.module.js > dist/AS.module.min.js",
    "build-dist": "rollup -c",
    "help": "npm run && rollup && squash"
  },
  "dependencies": {
    "dat.gui": "dataarts/dat.gui",
    "stats.js": "^0.17.0",
    "three": "latest"
  },
  "devDependencies": {
    "CoreAS": "backspaces/CoreAS",
    "butternut": "^0.4.6",
    "rollup": "latest",
    "sane": "^1.6.0"
  }
}
